# This is your personal gimprc file.  Any variable defined in this file takes
# precedence over the value defined in the system-wide gimprc:
# /etc/gimp/2.0/gimprc
# Most values can be set within GIMP by changing some options in the
# Preferences dialog.

(num-processors 8)
(tile-cache-size 1024M)
(brush-path "/home/mikhail/.config/GIMP/2.9/brushes")
(brush-path-writable "/home/mikhail/.config/GIMP/2.9/brushes")
(dynamics-path "/home/mikhail/.config/GIMP/2.9/dynamics")
(dynamics-path-writable "/home/mikhail/.config/GIMP/2.9/dynamics")
(pattern-path "/home/mikhail/.config/GIMP/2.9/patterns")
(pattern-path-writable "/home/mikhail/.config/GIMP/2.9/patterns")
(palette-path "/home/mikhail/.config/GIMP/2.9/palettes")
(palette-path-writable "/home/mikhail/.config/GIMP/2.9/palettes")
(font-path "/home/mikhail/.config/GIMP/2.9/fonts:/usr/share/gimp/2.0/fonts:/usr/share/fonts:/home/mikhail/Documents/Utilities/Fonts")
(global-brush no)
(global-pattern no)
(global-gradient no)
(default-image
    (width 2560)
    (height 1600)
    (unit pixels)
    (xresolution 300.000000)
    (yresolution 300.000000)
    (resolution-unit inches)
    (image-type rgb)
    (precision u32-gamma)
    (fill-type white-fill)
    (comment "All images credited to Tristen Lundquist\nMade with GIMP"))
(undo-size 1024M)
(layer-preview-size small)
(thumbnail-size none)
(color-management
    (mode display)
    (display-profile-from-gdk yes)
    (display-rendering-intent relative-colorimetric)
    (display-use-black-point-compensation yes)
    (simulation-rendering-intent perceptual)
    (simulation-use-black-point-compensation yes)
    (simulation-gamut-check no)
    (out-of-gamut-color (color-rgb 0.501961 0.501961 0.501961))
    (display-module "CdisplayLcms"))
(color-profile-policy keep)
(save-document-history no)
(quick-mask-color (color-rgba 0.000000 0.047059 1.000000 0.500000))
(cursor-mode crosshair)
(monitor-xresolution 142.000000)
(monitor-yresolution 141.000000)
(navigation-preview-size small)
(default-view
    (show-menubar no)
    (show-statusbar no)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary no)
    (show-guides no)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode custom)
    (padding-color (color-rgb 0.200000 0.200000 0.200000)))
(default-fullscreen-view
    (show-menubar no)
    (show-statusbar no)
    (show-rulers no)
    (show-scrollbars no)
    (show-selection yes)
    (show-layer-boundary no)
    (show-guides no)
    (show-grid no)
    (show-sample-points yes)
    (snap-to-guides yes)
    (snap-to-grid no)
    (snap-to-canvas no)
    (snap-to-path no)
    (padding-mode custom)
    (padding-color (color-rgb 0.200000 0.200000 0.200000)))
(save-tool-options yes)
(max-new-image-size 1536M)
(toolbox-color-area no)
(theme "Small")
